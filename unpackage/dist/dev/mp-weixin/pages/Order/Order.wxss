.box {
  padding: 0 40rpx;
  background-color: #FFFFFF;
}
.box .input-icon {
  margin-left: 20rpx;
}
.box .u-form-item__body {
  padding: 30rpx 0;
}
.box .popup-content {
  width: 551rpx;
  height: 836rpx;
  background-color: #FFFFFF;
  border-radius: 20rpx;
  padding: 50rpx 43rpx;
}
.box .popup-content .title {
  font-size: 30rpx;
  font-family: Source Han Sans CN;
  font-weight: 400;
  color: #000000;
  line-height: 30rpx;
  text-align: center;
  margin-bottom: 58rpx;
}
.box .popup-content .type-item {
  width: 465rpx;
  height: 145rpx;
  padding: 0 71rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border: 1px solid #ECF3FD;
  border-radius: 10rpx;
  margin-bottom: 40rpx;
  position: relative;
}
.box .popup-content .type-item .item-left {
  width: 80rpx;
  height: 80rpx;
  margin-right: 27rpx;
}
.box .popup-content .type-item .item-right {
  flex: 0 0 auto;
}
.box .popup-content .type-item .item-right .item-tip1 {
  font-size: 28rpx;
  font-family: Source Han Sans CN;
  font-weight: 400;
  color: #333333;
  line-height: 30rpx;
  margin-bottom: 15rpx;
}
.box .popup-content .type-item .item-right .item-tip2 {
  font-size: 22rpx;
  font-family: Source Han Sans CN;
  font-weight: 400;
  color: #999999;
  line-height: 30rpx;
}
.box .popup-content .type-item .type-item-img {
  position: absolute;
  bottom: 0;
  right: 0;
  width: 42rpx;
  height: 40rpx;
}
.box .popup-content .type-item-select {
  background: #FDF4EC;
  border: 1px solid #F79239;
}
.box .popup-content .btns {
  width: 379rpx;
  height: 60rpx;
  margin: 56rpx auto 0;
}
.box .popup2-content {
  width: 750rpx;
  height: 776rpx;
}
.box .popup2-content .title {
  width: 750rpx;
  height: 100rpx;
  background: #3187F8;
  font-size: 30rpx;
  font-family: Source Han Sans CN;
  font-weight: 400;
  color: #FFFFFF;
  border-radius: 20rpx 20rpx 0px 0px;
  display: flex;
  align-items: center;
  justify-content: center;
}
.box .popup2-content .box {
  padding: 39rpx 23rpx 0;
}
.box .popup2-content .box .tip {
  font-size: 24rpx;
  font-family: Source Han Sans CN;
  font-weight: 400;
  color: #666666;
  line-height: 30rpx;
}
.box .popup2-content .box .box-type {
  margin-top: 32rpx;
  display: flex;
  flex-flow: row wrap;
}
.box .popup2-content .box .box-type .type-item {
  width: 25%;
  height: 46rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-bottom: 29rpx;
}
.box .popup2-content .box .box-type .type-item > view {
  width: 140rpx;
  height: 46rpx;
  border: 1rpx solid #CCCCCC;
  border-radius: 6rpx;
  font-size: 24rpx;
  font-family: Source Han Sans CN;
  font-weight: 400;
  color: #666666;
  display: flex;
  align-items: center;
  justify-content: center;
}
.box .popup2-content .box .box-type .type-item .type-item-select {
  color: #EDF4FE;
  background: linear-gradient(91deg, #64A6FC, #3187F8);
}
.box .popup2-content .btns {
  width: 379rpx;
  height: 70rpx;
  margin: 44rpx auto 0;
}
.box .psec .u-icon__icon {
  color: #FFFFFF !important;
}

